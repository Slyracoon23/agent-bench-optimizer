{
  "name": "agent-bench",
  "version": "0.1.0",
  "description": "Framework for testing and benchmarking AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-bench": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "dev": "vitest",
    "examples": "vitest run examples/customer-service.test.ts",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "agent-bench": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.13",
    "ai": "^4.1.45",
    "commander": "^13.1.0",
    "common-tags": "^1.8.2",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.7",
    "js-yaml": "^4.1.0",
    "openai": "^4.85.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/dedent": "^0.7.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "keywords": [
    "ai",
    "testing",
    "benchmarking",
    "agents",
    "llm"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "",
  "license": "MIT"
}